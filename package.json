{
  "publisher": "vtrbo",
  "name": "dot-thing",
  "displayName": "Dot Thing",
  "version": "0.0.8",
  "packageManager": "pnpm@7.7.0",
  "description": "This is a Vscode plugin passing symbol. to type content quickly.",
  "author": "Victor Bo<vtrbo@qq.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/vtrbo",
  "homepage": "https://github.com/vtrbo/dot-thing#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/vtrbo/dot-thing"
  },
  "bugs": {
    "url": "https://github.com/vtrbo/dot-thing/issues"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/vtrbo"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.69.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "configuration": {
      "title": "DotThing",
      "properties": {
        "dotThing.languages": {
          "type": "array",
          "description": "DotThing 支持的语言"
        },
        "dotThing.options": {
          "type": "array",
          "description": "DotThing 键入的格式"
        }
      }
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp --commit --push --tag && nr publish"
  },
  "devDependencies": {
    "@babel/types": "^7.18.9",
    "@types/node": "^18.6.2",
    "@types/vscode": "^1.69.0",
    "@vtrbo/eslint-config": "^0.0.6",
    "@vtrbo/ni": "^0.0.3",
    "bumpp": "^8.2.1",
    "eslint": "^8.20.0",
    "esno": "^0.16.3",
    "pnpm": "^7.7.0",
    "rimraf": "^3.0.2",
    "tsup": "^6.2.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vitest": "^0.19.1",
    "vsce": "^2.10.0"
  }
}
